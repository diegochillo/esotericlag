@prefix : <http://example.org/esoteric-artifacts#> .
@prefix fred: <http://www.ontologydesignpatterns.org/ont/fred/domain.owl#> .
@prefix d0: <http://www.ontologydesignpatterns.org/ont/d0.owl#> .

fred:enigma_2 a :EsotericArtifact ;
    :exceedsConventionalForm true ;
    :hasMaterialDurability true ;
    :hasPermanence true ;
    :operatesAcrossGenerations true ;
    :requiresKnowledge :LatinScholarship ;
    :requiresFramework :HumanistInterpretiveFramework ;
    :generatesInterpretiveCommunity :RenaissanceScholarCommunity ;
    :createsHierarchy :ScholarlySocialHierarchy ;
    :mediatesRelationship :ScholarlyNetwork ;
    :reproducesStructure :HumanistSocialStructure ;
    :pointsTo :DeathDeity , :OntologicalMystery ;
    :transcendsBoundary :GenderBoundary , :AgeBoundary , :MoralBoundary , 
                        :SpatialBoundary , :MonumentTypeBoundary , :LifeDeathBoundary ;
    :blursBoundary :GenderBoundary , :AgeBoundary , :MoralBoundary , 
                   :SpatialBoundary , :MonumentTypeBoundary , :LifeDeathBoundary .

