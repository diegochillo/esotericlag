@prefix : <http://example.org/ontology/jungian-artifact-analysis#> .
@prefix d0: <http://www.ontologydesignpatterns.org/ont/d0.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix fred: <http://www.ontologydesignpatterns.org/ont/fred/domain.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

fred:cross_1 a :EsotericArtifact ;
    :emergesFrom :counterReformationContext ;
    :expressesOpposite :solarMasculineOpposite , :lunarFeminineOpposite , :spiritualOpposite , :materialOpposite ;
    :projectsArchetype :selfArchetype_1 ;
    :containsArchetype :selfArchetype_1 ;
    :containsPrimordialImage :primordialSun , :primordialMoon , :primordialCross , :primordialEye ;
    :hasStructure :quaternaryStructure_1 ;
    :expressesIndividuation :individuationProcess_1 ;
    :expressesCompensation :compensatoryReaction_1 ;
    :requiresAttitude :symbolicConsciousness_1 ;
    :demandsTransformation :psychologicalTransformation_1 ;
    :hasParallel :alchemicalMarriage , :christianQuaternityParallel .

:counterReformationContext a :CollectiveContext ;
    rdfs:label "Counter-Reformation Era Context" ;
    rdfs:comment "17th century European religious and cultural crisis requiring reconciliation of Christian orthodoxy with esoteric wisdom traditions" .

:solarMasculineOpposite a :PsychicOpposite ;
    :representedBy fred:sun_1 ;
    :pairsWith :lunarFeminineOpposite ;
    rdfs:label "Solar-Masculine Principle" ;
    rdfs:comment "Conscious, active, rational principle" .

:lunarFeminineOpposite a :PsychicOpposite ;
    :representedBy fred:moon_1 ;
    :pairsWith :solarMasculineOpposite ;
    rdfs:label "Lunar-Feminine Principle" ;
    rdfs:comment "Unconscious, receptive, intuitive principle" .

:spiritualOpposite a :PsychicOpposite ;
    :representedBy fred:cross_2 ;
    :pairsWith :materialOpposite ;
    rdfs:label "Spiritual Principle" ;
    rdfs:comment "Transcendent, heavenly realm" .

:materialOpposite a :PsychicOpposite ;
    :representedBy fred:base_2 ;
    :pairsWith :spiritualOpposite ;
    rdfs:label "Material Principle" ;
    rdfs:comment "Earthly, embodied realm" .

:selfArchetype_1 a :SelfArchetype ;
    :holdsOppositesInTension :solarMasculineOpposite , :lunarFeminineOpposite , :spiritualOpposite , :materialOpposite ;
    :manifestsIn fred:section_1 ;
    rdfs:label "Self Archetype in Hendaye Cross" ;
    rdfs:comment "The organizing center manifested through quaternary structure" .

:primordialSun a :PrimordialImage ;
    :representedBy fred:sun_1 ;
    :foundIn :alchemyTradition , :christianTradition ;
    rdfs:label "Primordial Sun Image" ;
    rdfs:comment "Universal symbol of consciousness and divine illumination" .

:primordialMoon a :PrimordialImage ;
    :representedBy fred:moon_1 ;
    :foundIn :alchemyTradition , :paganTradition ;
    rdfs:label "Primordial Moon Image" ;
    rdfs:comment "Universal symbol of the unconscious and receptive principle" .

:primordialCross a :PrimordialImage ;
    :representedBy fred:cross_1 ;
    :foundIn :christianTradition , :mandalaTradition ;
    rdfs:label "Primordial Cross/Quaternity Image" ;
    rdfs:comment "Universal symbol of totality and orientation in psychic space" .

:primordialEye a :PrimordialImage ;
    :representedBy fred:eye_1 , fred:eye_2 ;
    :foundIn :egyptianTradition , :gnosticTradition ;
    rdfs:label "Primordial Eye Image" ;
    rdfs:comment "Universal symbol of consciousness and divine vision" .

:quaternaryStructure_1 a :QuaternaryStructure ;
    dul:associatedWith fred:section_1 ;
    :expressesTotality ;
    rdfs:label "Fourfold Structure of Base" ;
    rdfs:comment "The four sections containing symbols express the Self archetype's completeness" .

:individuationProcess_1 a :Individuation ;
    :synthesizes :solarMasculineOpposite , :lunarFeminineOpposite ;
    :achieves :coniunctio_1 ;
    rdfs:label "Individuation Process Symbolized" ;
    rdfs:comment "The movement toward psychic wholeness through integration of opposites" .

:compensatoryReaction_1 a :CompensatoryReaction ;
    :compensatesFor :rationalDogmatismAttitude ;
    rdfs:label "Compensatory Esoteric Symbolism" ;
    rdfs:comment "Unconscious compensation for one-sided religious rationalism through symbolic multiplicity" .

:rationalDogmatismAttitude a :ConsciousAttitude ;
    rdfs:label "Counter-Reformation Rational Dogmatism" ;
    rdfs:comment "The prevailing conscious attitude of theological literalism and doctrinal rigidity" .

:symbolicConsciousness_1 a :SymbolicConsciousness ;
    rdfs:label "Required Symbolic Consciousness" ;
    rdfs:comment "The psychological attitude necessary to perceive deeper archetypal meanings beyond literal religious interpretation" .

:psychologicalTransformation_1 a :PsychologicalTransformation ;
    :abandons :literalism_1 ;
    :produces :integratedConsciousness ;
    rdfs:label "Transformation Demanded" ;
    rdfs:comment "Movement from literal to symbolic understanding, integrating conscious and unconscious" .

:literalism_1 a :Literalism ;
    rdfs:label "Religious Literalism" ;
    rdfs:comment "Surface-level interpretation that must be transcended" .

:integratedConsciousness a :ConsciousAttitude ;
    rdfs:label "Integrated Consciousness" ;
    rdfs:comment "Consciousness that holds opposites in creative tension" .

:coniunctio_1 a :Coniunctio ;
    :unites :solarMasculineOpposite , :lunarFeminineOpposite ;
    rdfs:label "Alchemical Marriage" ;
    rdfs:comment "The synthesis of solar and lunar principles in the Self" .

:alchemicalMarriage a :MythologicalParallel ;
    rdfs:label "Alchemical Marriage Parallel" ;
    rdfs:comment "Correspondence with the alchemical coniunctio of sun and moon" .

:christianQuaternityParallel a :ReligiousParallel ;
    rdfs:label "Christian Quaternity Parallel" ;
    rdfs:comment "Correspondence with Christian symbols of totality and the four evangelists" .

:alchemyTradition a :EsotericTradition ;
    rdfs:label "Alchemical Tradition" .

:christianTradition a :EsotericTradition ;
    rdfs:label "Christian Esoteric Tradition" .

:paganTradition a :EsotericTradition ;
    rdfs:label "Pagan Tradition" .

:egyptianTradition a :EsotericTradition ;
    rdfs:label "Egyptian Tradition" .

:gnosticTradition a :EsotericTradition ;
    rdfs:label "Gnostic Tradition" .

:mandalaTradition a :EsotericTradition ;
    rdfs:label "Mandala Tradition" .

fred:eye_1 :manifestsArchetype :consciousnessArchetype .

fred:eye_2 :manifestsArchetype :consciousnessArchetype .

:consciousnessArchetype a :Archetype ;
    rdfs:label "Consciousness Archetype" ;
    rdfs:comment "The principle of awareness and psychic illumination" .

fred:spiral_1 a :Paradox ;
    :activates :transcendentFunction_1 ;
    rdfs:label "Spiral Paradox" ;
    rdfs:comment "Movement simultaneously inward and outward, representing transformation" .

:transcendentFunction_1 a :TranscendentFunction ;
    :mediates :solarMasculineOpposite , :lunarFeminineOpposite ;
    :produces :psychologicalTransformation_1 ;
    rdfs:label "Transcendent Function Activated" ;
    rdfs:comment "The psychic function that unites opposites into a third position" .

fred:string-entity_1 :symbolizesTransformation ;
    :containsArchetype :deathRebirthArchetype .

:deathRebirthArchetype a :Archetype ;
    rdfs:label "Death-Rebirth Archetype" ;
    rdfs:comment "INRI as transformation formula: Igne Natura Renovatur Integra" .

fred:product_1 :representsDoubling ;
    :symbolizesOpposition .

:totalityQuality a :Totality ;
    :encompasses :solarMasculineOpposite , :lunarFeminineOpposite , :spiritualOpposite , :materialOpposite ;
    dul:associatedWith fred:cross_1 .

fred:section_1 :expressesTotalityThrough :quaternaryStructure_1 .