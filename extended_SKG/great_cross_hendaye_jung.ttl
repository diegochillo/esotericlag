@prefix : <http://example.org/ontology/jungian-artifact-analysis#> .
@prefix d0: <http://www.ontologydesignpatterns.org/ont/d0.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix fred: <http://www.ontologydesignpatterns.org/ont/fred/domain.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Classify the Cross as an Esoteric Artifact
fred:cross_1 a :EsotericArtifact .

# Historical and Cultural Context (META-QUESTION)
fred:cross_1 :emergesFrom :CounterReformationContext .

:CounterReformationContext a :CollectiveContext ;
    rdfs:label "Counter-Reformation European Context" ;
    rdfs:comment "Period of religious crisis and alchemical revival in 17th century Europe" ;
    :respondsTo :ReligiousPsychologicalCrisis .

:ReligiousPsychologicalCrisis a :CollectivePsychologicalNeed ;
    rdfs:comment "Crisis between dogmatic faith and emerging rationalism requiring symbolic reconciliation" .

# Psychic Opposites (Question 1)
fred:cross_1 :expressesOpposite :SunMoonOpposite ;
    :expressesOpposite :SpiritMatterOpposite ;
    :expressesOpposite :ConsciousUnconsciousOpposite ;
    :expressesOpposite :MasculineFeminineOpposite .

:SunMoonOpposite a :PsychicOpposite ;
    :constitutedBy fred:sun_1 , fred:moon_1 ;
    :representsOpposition :ConsciousPrinciple , :UnconsciousPrinciple ;
    :pointsToward :Coniunctio_1 .

:ConsciousPrinciple a :PsychicOpposite ;
    :embodiedIn fred:sun_1 ;
    rdfs:comment "Solar consciousness, rational ego awareness" .

:UnconsciousPrinciple a :PsychicOpposite ;
    :embodiedIn fred:moon_1 ;
    rdfs:comment "Lunar unconscious, receptive psyche" .

:SpiritMatterOpposite a :PsychicOpposite ;
    :constitutedBy fred:cross_3 , fred:base_2 ;
    rdfs:comment "Opposition between upper cross (spiritual realm) and pedestal base (material foundation)" .

:ConsciousUnconsciousOpposite a :PsychicOpposite ;
    :manifestsThrough fred:eye_1 , fred:eye_2 .

:MasculineFeminineOpposite a :PsychicOpposite ;
    :constitutedBy fred:sun_1 , fred:moon_1 ;
    rdfs:comment "Archetypal masculine (sun with 8 rays) and feminine (crescent moon)" .

:Coniunctio_1 a :Coniunctio ;
    :unites :SunMoonOpposite ;
    :symbolizedBy fred:base_2 ;
    rdfs:comment "The sacred marriage of opposites symbolized in the quaternary base structure" .

# Unconscious Contents and Projection (Question 2)
fred:cross_1 :projectsArchetype :SelfArchetype_1 ;
    :projectsArchetype :QuaternityArchetype ;
    :projectsArchetype :CosmicOrderArchetype ;
    :expressesCompensation :CompensatoryReaction_1 .

:SelfArchetype_1 a :SelfArchetype ;
    :manifestsIn fred:base_2 ;
    :holdsOppositesInTension :SunMoonOpposite .

:QuaternityArchetype a :Archetype ;
    :manifestsIn fred:section_1 ;
    rdfs:comment "Archetype of fourfold wholeness" .

:CosmicOrderArchetype a :Archetype ;
    :manifestsIn fred:star_1 ;
    rdfs:comment "Eight-rayed star representing cosmic harmony and celestial order" .

:CompensatoryReaction_1 a :CompensatoryReaction ;
    :compensatesFor :DogmaticChristianity ;
    rdfs:comment "Unconscious compensation for one-sided religious rationalism through symbolic quaternity" .

:DogmaticChristianity a :ConsciousAttitude ;
    rdfs:comment "Prevailing conscious attitude of literalist Christian theology" .

fred:cross_1 :compensatesFor :DogmaticChristianity .

# Self Archetype and Individuation (Question 3)
fred:cross_1 :expressesTotality :Totality_1 ;
    :expressesIndividuation :IndividuationProcess_1 ;
    :hasStructure :QuaternaryStructure_1 ;
    :hasStructure :MandalaForm_1 .

:Totality_1 a :Totality ;
    :encompasses :SunMoonOpposite ;
    :encompasses :SpiritMatterOpposite ;
    rdfs:comment "Wholeness achieved through integration of all four symbols on pedestal base" .

:IndividuationProcess_1 a :Individuation ;
    :expressingMovementToward :SelfRealization ;
    rdfs:comment "The movement toward psychic wholeness through contemplation of the quaternary symbols" .

:SelfRealization a d0:Event ;
    rdfs:comment "Realization of the Self through symbolic integration" .

:QuaternaryStructure_1 a :QuaternaryStructure ;
    :embodiedIn fred:section_1 ;
    :containsElements fred:star_1 , fred:moon_1 , fred:sun_1 , fred:oval_1 ;
    rdfs:comment "Four-fold division with four distinct symbols expressing completeness" .

:MandalaForm_1 a :MandalaForm ;
    :embodiedIn fred:base_2 ;
    rdfs:comment "Circular arrangement of quaternary symbols functions as mandala" .

# Primordial Images (Question 4)
fred:cross_1 :containsPrimordialImage :SolarImage ;
    :containsPrimordialImage :LunarImage ;
    :containsPrimordialImage :CrossImage ;
    :containsPrimordialImage :StarImage ;
    :containsPrimordialImage :QuaternityImage .

:SolarImage a :PrimordialImage ;
    :manifestsIn fred:sun_1 ;
    :foundIn :AlchemicalTradition , :MythologicalSolarCults ;
    :hasParallel :SolInvictusParallel ;
    rdfs:comment "Universal solar symbolism of consciousness and masculine principle" .

:LunarImage a :PrimordialImage ;
    :manifestsIn fred:moon_1 ;
    :foundIn :AlchemicalTradition , :GoddessReligions ;
    :hasParallel :LunarDeitiesParallel ;
    rdfs:comment "Universal lunar symbolism of unconscious and feminine receptivity" .

:CrossImage a :PrimordialImage ;
    :manifestsIn fred:cross_1 ;
    :foundIn :ChristianTradition , :MandalaTradition ;
    :hasParallel :QuaternitySymbolsParallel ;
    rdfs:comment "Archetypal cross as axis mundi and union of opposites" .

:StarImage a :PrimordialImage ;
    :manifestsIn fred:star_1 ;
    :foundIn :HermeticTradition , :AstrologyTradition ;
    rdfs:comment "Eight-rayed star as cosmic order and celestial perfection" .

:QuaternityImage a :PrimordialImage ;
    :embodiedIn :QuaternaryStructure_1 ;
    :foundIn :AlchemicalTradition , :MandalaTradition , :IndigenousTraditions ;
    :hasParallel :FourElementsParallel , :FourDirectionsParallel ;
    rdfs:comment "Universal four-fold pattern expressing totality and orientation" .

:AlchemicalTradition a :EsotericTradition ;
    rdfs:label "Alchemical Tradition" .

:MythologicalSolarCults a :EsotericTradition ;
    rdfs:label "Mythological Solar Traditions" .

:GoddessReligions a :EsotericTradition ;
    rdfs:label "Goddess and Lunar Religions" .

:ChristianTradition a :EsotericTradition ;
    rdfs:label "Christian Symbolic Tradition" .

:MandalaTradition a :EsotericTradition ;
    rdfs:label "Mandala Traditions" .

:HermeticTradition a :EsotericTradition ;
    rdfs:label "Hermetic Tradition" .

:AstrologyTradition a :EsotericTradition ;
    rdfs:label "Astrological Tradition" .

:IndigenousTraditions a :EsotericTradition ;
    rdfs:label "Indigenous Quaternary Symbolism" .

:SolInvictusParallel a :MythologicalParallel ;
    rdfs:comment "Parallel to Sol Invictus and solar deities across cultures" .

:LunarDeitiesParallel a :MythologicalParallel ;
    rdfs:comment "Parallel to lunar goddesses (Diana, Selene, Hecate)" .

:QuaternitySymbolsParallel a :ReligiousParallel ;
    rdfs:comment "Parallel to quaternary symbols in Christian, Buddhist, and indigenous traditions" .

:FourElementsParallel a :MythologicalParallel ;
    rdfs:comment "Parallel to four classical elements across cultures" .

:FourDirectionsParallel a :MythologicalParallel ;
    rdfs:comment "Parallel to four cardinal directions in indigenous cosmologies" .

# Conscious Attitude and Transformation (Question 5)
fred:cross_1 :requiresAttitude :SymbolicConsciousness_1 ;
    :demandsTransformation :PsychologicalTransformation_1 ;
    :engagesParadox :Paradox_1 .

:SymbolicConsciousness_1 a :SymbolicConsciousness ;
    :transcends :Literalism_1 ;
    rdfs:comment "Required symbolic mode of perception beyond literal Christian interpretation" .

:Literalism_1 a :Literalism ;
    rdfs:comment "Inadequate literal interpretation limited to INRI inscription and surface Christian symbolism" .

:PsychologicalTransformation_1 a :PsychologicalTransformation ;
    :abandons :EgoConsciousness_1 ;
    :realizesArchetype :SelfArchetype_1 ;
    :achieves :IntegrationOfOpposites ;
    rdfs:comment "Transformation from ego-consciousness to Self-realization through contemplation" .

:EgoConsciousness_1 a :EgoConsciousness ;
    rdfs:comment "Limited ego perspective transcended through symbolic engagement" .

:IntegrationOfOpposites a d0:Event ;
    rdfs:comment "Psychological integration achieved through holding the tension of opposites" .

:Paradox_1 a :Paradox ;
    :activates :TranscendentFunction_1 ;
    :embodiedIn :ChristianAlchemicalTension ;
    rdfs:comment "Paradox of Christian cross bearing alchemical-hermetic quaternary symbolism" .

:ChristianAlchemicalTension a dul:Quality ;
    rdfs:comment "Tension between orthodox Christian symbolism and heterodox alchemical content" .

:TranscendentFunction_1 a :TranscendentFunction ;
    :mediates :SunMoonOpposite ;
    :mediates :SpiritMatterOpposite ;
    :produces :PsychologicalTransformation_1 ;
    rdfs:comment "Mediating function activated by contemplating the opposites held in the artifact" .

# Additional Jungian Observations
fred:eye_1 :representsSymbol :SpiralDynamism .

:SpiralDynamism a :PrimordialImage ;
    rdfs:comment "Spiral eye represents dynamic unconscious process, circumambulation toward center" .

fred:eye_2 :representsSymbol :OctopartiteStructure .

:OctopartiteStructure a :PrimordialImage ;
    rdfs:comment "Eight rays/eyes represent eightfold cosmic structure and mandala expansion" .

fred:string-entity_1 :embodiesParadox :INRIParadox .

:INRIParadox a :Paradox ;
    rdfs:comment "INRI inscription connects crucifixion (death) with alchemical regeneration" .

fred:phrase_1 :expressesArchetype :HopeInSacrifice .

:HopeInSacrifice a :Archetype ;
    rdfs:comment "O CRUX AVE SPES UNICA - archetype of redemption through suffering and transformation" .

fred:oval_1 :manifestsSymbol :AlchemicalVessel .

:AlchemicalVessel a :PrimordialImage ;
    rdfs:comment "Oval as vas Hermeticum, containing vessel for transformation of quaternary elements" .