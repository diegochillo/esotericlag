@prefix : <http://example.org/guenon#> .
@prefix d0: <http://www.ontologydesignpatterns.org/ont/d0.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/guenon#> .

<http://example.org/guenon> rdf:type owl:Ontology ;
                             owl:imports <http://www.ontologydesignpatterns.org/ont/d0.owl> ;
                             rdfs:comment "Vocabulary for analyzing esoteric artifacts according to René Guénon's traditional perspective"@en ;
                             rdfs:label "Guenonian Artifact Analysis Ontology"@en .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/guenon#connectsTo
:connectsTo rdf:type owl:ObjectProperty ;
            rdfs:domain :EsotericArtifact ;
            rdfs:range :MetaphysicalPrinciple .


###  http://example.org/guenon#containsSymbol
:containsSymbol rdf:type owl:ObjectProperty ;
                rdfs:domain :EsotericArtifact ;
                rdfs:range :Symbol ;
                rdfs:comment "Indicates a symbol present in the artifact"@en ;
                rdfs:label "contains symbol"@en .


###  http://example.org/guenon#documentedIn
:documentedIn rdf:type owl:ObjectProperty ;
              rdfs:domain :Symbol ;
              rdfs:range :TraditionalSource ;
              rdfs:comment "Links a symbol to its documentation in traditional sources"@en ;
              rdfs:label "documented in"@en .


###  http://example.org/guenon#employsCorrespondence
:employsCorrespondence rdf:type owl:ObjectProperty ;
                       rdfs:domain :EsotericArtifact ;
                       rdfs:range :Correspondence ;
                       rdfs:comment "Indicates use of analogical correspondence"@en ;
                       rdfs:label "employs correspondence"@en .


###  http://example.org/guenon#hasAuthenticityStatus
:hasAuthenticityStatus rdf:type owl:ObjectProperty ;
                       rdfs:domain :EsotericArtifact ;
                       rdfs:range :AuthenticityStatus ;
                       rdfs:comment "Assigns authenticity classification to an artifact"@en ;
                       rdfs:label "has authenticity status"@en .


###  http://example.org/guenon#hasEsotericLayer
:hasEsotericLayer rdf:type owl:ObjectProperty ;
                  rdfs:domain :EsotericArtifact ;
                  rdfs:range :EsotericLayer ;
                  rdfs:comment "Indicates inner initiatic meaning layer"@en ;
                  rdfs:label "has esoteric layer"@en .


###  http://example.org/guenon#hasExegesis
:hasExegesis rdf:type owl:ObjectProperty ;
             rdfs:domain :EsotericArtifact ;
             rdfs:range :Exegesis ;
             rdfs:comment "Links artifact to interpretive commentary"@en ;
             rdfs:label "has exegesis"@en .


###  http://example.org/guenon#hasExotericLayer
:hasExotericLayer rdf:type owl:ObjectProperty ;
                  rdfs:domain :EsotericArtifact ;
                  rdfs:range :ExotericLayer ;
                  rdfs:comment "Indicates outer meaning layer"@en ;
                  rdfs:label "has exoteric layer"@en .


###  http://example.org/guenon#hasQuality
:hasQuality rdf:type owl:ObjectProperty ;
            rdfs:domain :EsotericArtifact ;
            rdfs:range :Quality ;
            rdfs:comment "Relates artifact to its essential qualitative aspects"@en ;
            rdfs:label "has quality"@en .


###  http://example.org/guenon#originatesFrom
:originatesFrom rdf:type owl:ObjectProperty ;
                rdfs:domain :EsotericArtifact ;
                rdfs:range :TraditionalLineage ;
                rdfs:comment "Connects an artifact to its traditional lineage of origin"@en ;
                rdfs:label "originates from"@en .


###  http://example.org/guenon#supportsIntellectualIntuition
:supportsIntellectualIntuition rdf:type owl:ObjectProperty ;
                               rdfs:domain :Symbol ;
                               rdfs:range <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent> ;
                               rdfs:comment "Indicates a symbol's capacity to support supra-rational knowledge"@en ;
                               rdfs:label "supports intellectual intuition"@en .


###  http://example.org/guenon#traceableTo
:traceableTo rdf:type owl:ObjectProperty ;
             rdfs:domain :Quality ;
             rdfs:range :TraditionalSource ;
             rdfs:comment "Links quality to its traditional source documentation"@en ;
             rdfs:label "traceable to"@en .


#################################################################
#    Data properties
#################################################################

###  http://example.org/guenon#connectionUncertain
:connectionUncertain rdf:type owl:DatatypeProperty ;
                     rdfs:domain :connectsTo ;
                     rdfs:range xsd:boolean ;
                     rdfs:comment "Indicates uncertainty in connection to metaphysical principles"@en ;
                     rdfs:label "connection uncertain"@en .


###  http://example.org/guenon#hasDateOfCreation
:hasDateOfCreation rdf:type owl:DatatypeProperty ;
                   rdfs:domain :EsotericArtifact ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Approximate or documented date of artifact creation"@en ;
                   rdfs:label "has date of creation"@en .


###  http://example.org/guenon#hasInscription
:hasInscription rdf:type owl:DatatypeProperty ;
                rdfs:domain :EsotericArtifact ;
                rdfs:range xsd:string ;
                rdfs:comment "Text inscribed on the artifact"@en ;
                rdfs:label "has inscription"@en .


###  http://example.org/guenon#isDocumented
:isDocumented rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Correspondence
                                          :EsotericLayer
                                          :Symbol
                                        )
                          ] ;
              rdfs:range xsd:boolean ;
              rdfs:comment "Indicates presence of traditional documentation"@en ;
              rdfs:label "is documented"@en .


###  http://example.org/guenon#isModernImposition
:isModernImposition rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Exegesis ;
                    rdfs:range xsd:boolean ;
                    rdfs:comment "Indicates interpretation imposed by modern commentators"@en ;
                    rdfs:label "is modern imposition"@en .


###  http://example.org/guenon#isSpeculative
:isSpeculative rdf:type owl:DatatypeProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( :Correspondence
                                           :Exegesis
                                         )
                           ] ;
               rdfs:range xsd:boolean ;
               rdfs:comment "Indicates whether interpretation lacks documented traditional basis"@en ;
               rdfs:label "is speculative"@en .


###  http://example.org/guenon#lacksTraditionalJustification
:lacksTraditionalJustification rdf:type owl:DatatypeProperty ;
                               rdfs:domain :Symbol ;
                               rdfs:range xsd:boolean ;
                               rdfs:comment "Combination or use lacks documented traditional rationale"@en ;
                               rdfs:label "lacks traditional justification"@en .


#################################################################
#    Classes
#################################################################

###  http://example.org/guenon#AuthenticityStatus
:AuthenticityStatus rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Abstract> ;
                    rdfs:comment "Classification of an artifact's relationship to authentic tradition"@en ;
                    rdfs:label "Authenticity Status"@en .


###  http://example.org/guenon#Correspondence
:Correspondence rdf:type owl:Class ;
                rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Abstract> ;
                rdfs:comment "Analogical relationship between different orders of reality"@en ;
                rdfs:label "Correspondence"@en .


###  http://example.org/guenon#EsotericArtifact
:EsotericArtifact rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#DesignedArtifact> ;
                  rdfs:comment "A physical object subject to traditional esoteric analysis"@en ;
                  rdfs:label "Esoteric Artifact"@en .


###  http://example.org/guenon#EsotericLayer
:EsotericLayer rdf:type owl:Class ;
               rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ;
               rdfs:comment "Inner, initiatic meaning accessible only through authentic transmission"@en ;
               rdfs:label "Esoteric Layer"@en .


###  http://example.org/guenon#Exegesis
:Exegesis rdf:type owl:Class ;
          rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ;
          rdfs:comment "Interpretation or commentary on an artifact or symbol"@en ;
          rdfs:label "Exegesis"@en .


###  http://example.org/guenon#ExotericLayer
:ExotericLayer rdf:type owl:Class ;
               rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ;
               rdfs:comment "Outer, publicly accessible meaning"@en ;
               rdfs:label "Exoteric Layer"@en .


###  http://example.org/guenon#MacrocosmMicrocosmCorrespondence
:MacrocosmMicrocosmCorrespondence rdf:type owl:Class ;
                                  rdfs:subClassOf :Correspondence ;
                                  rdfs:comment "Traditional correspondence between universal and individual orders"@en ;
                                  rdfs:label "Macrocosm-Microcosm Correspondence"@en .


###  http://example.org/guenon#MetaphysicalPrinciple
:MetaphysicalPrinciple rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Abstract> ;
                       rdfs:comment "Universal, transcendent principle according to traditional metaphysics"@en ;
                       rdfs:label "Metaphysical Principle"@en .


###  http://example.org/guenon#ModernSymbol
:ModernSymbol rdf:type owl:Class ;
              rdfs:subClassOf :Symbol ;
              owl:disjointWith :TraditionalSymbol ;
              rdfs:comment "Symbol of modern invention or personal creation"@en ;
              rdfs:label "Modern Symbol"@en .


###  http://example.org/guenon#Quality
:Quality rdf:type owl:Class ;
         rdfs:subClassOf d0:Quality ;
         rdfs:comment "Essential qualitative aspect as distinguished from quantitative"@en ;
         rdfs:label "Quality"@en .


###  http://example.org/guenon#Symbol
:Symbol rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ;
        rdfs:comment "A symbolic form or representation"@en ;
        rdfs:label "Symbol"@en .


###  http://example.org/guenon#TraditionalLineage
:TraditionalLineage rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept> ;
                    rdfs:comment "An authentic transmission chain of initiatic or traditional knowledge"@en ;
                    rdfs:label "Traditional Lineage"@en .


###  http://example.org/guenon#TraditionalSource
:TraditionalSource rdf:type owl:Class ;
                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject> ;
                   rdfs:comment "Documented traditional text or teaching"@en ;
                   rdfs:label "Traditional Source"@en .


###  http://example.org/guenon#TraditionalSymbol
:TraditionalSymbol rdf:type owl:Class ;
                   rdfs:subClassOf :Symbol ;
                   rdfs:comment "Symbol documented in authentic traditional sources"@en ;
                   rdfs:label "Traditional Symbol"@en .


###  http://example.org/guenon#connectsTo
:connectsTo rdf:type owl:Class ;
            rdfs:subClassOf <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action> .


###  http://www.ontologydesignpatterns.org/ont/d0.owl#Quality
d0:Quality rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://example.org/guenon#Authentic
:Authentic rdf:type owl:NamedIndividual ,
                    :AuthenticityStatus ;
           rdfs:comment "Originating from documented traditional lineage"@en ;
           rdfs:label "Authentic"@en .


###  http://example.org/guenon#Profane
:Profane rdf:type owl:NamedIndividual ,
                  :AuthenticityStatus ;
         rdfs:comment "Non-sacred, without esoteric significance"@en ;
         rdfs:label "Profane"@en .


###  http://example.org/guenon#PseudoTraditional
:PseudoTraditional rdf:type owl:NamedIndividual ,
                            :AuthenticityStatus ;
                   rdfs:comment "Modern creation presenting as traditional"@en ;
                   rdfs:label "Pseudo-Traditional"@en .


###  http://example.org/guenon#Questionable
:Questionable rdf:type owl:NamedIndividual ,
                       :AuthenticityStatus ;
              rdfs:comment "Lacking clear connection to traditional lineage"@en ;
              rdfs:label "Questionable"@en .


#################################################################
#    Annotations
#################################################################

:connectsTo rdfs:comment "Relates an artifact to metaphysical principles"@en ;
            rdfs:label "connects to"@en .


