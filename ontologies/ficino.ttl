@prefix : <http://www.semanticweb.org/ontologies/ficinian#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dolce: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

# Ontology Metadata
: a owl:Ontology ;
    dc:title "Ficinian Esoteric Interpretation Ontology - Core" ;
    dc:description "A synthesized semantic vocabulary for analyzing esoteric artifacts through Marsilio Ficino's interpretive framework, containing only elements common to multiple artifact types" ;
    dc:creator "Synthesized from interpretations of Aelia Laelia Crispis, Great Cross of Hendaye, and Flammarion Engraving" ;
    owl:imports <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
    owl:versionInfo "1.0 - Core synthesis of common elements" .

#################################################################
# Core Classes - Artifacts and Interpretations
#################################################################

:EsotericArtifact a owl:Class ;
    rdfs:subClassOf dolce:InformationObject ;
    rdfs:label "Esoteric Artifact" ;
    rdfs:comment "A physical or textual object containing encoded spiritual or philosophical wisdom" .

:FicinianInterpretation a owl:Class ;
    rdfs:subClassOf dolce:Description ;
    rdfs:label "Ficinian Interpretation" ;
    rdfs:comment "An analysis of an artifact through Ficino's five-fold hermeneutic lens" .

:InterpretiveLens a owl:Class ;
    rdfs:subClassOf dolce:Method ;
    rdfs:label "Interpretive Lens" ;
    rdfs:comment "One of the five hermeneutic perspectives in Ficino's framework" .

#################################################################
# The Five Interpretive Lenses (Ficino's Heuristic)
#################################################################

:PriscaTheologiaLens a owl:Class ;
    rdfs:subClassOf :InterpretiveLens ;
    rdfs:label "Prisca Theologia Lens" ;
    rdfs:comment "Interpretation through ancient wisdom and perennial philosophy" .

:NeoPlatonismLens a owl:Class ;
    rdfs:subClassOf :InterpretiveLens ;
    rdfs:label "Neo-Platonism Lens" ;
    rdfs:comment "Interpretation through Neo-Platonic cosmology and metaphysics" .

:ChristianIntegrationLens a owl:Class ;
    rdfs:subClassOf :InterpretiveLens ;
    rdfs:label "Christian Integration Lens" ;
    rdfs:comment "Interpretation emphasizing synthesis of pagan philosophy with Christian faith" .

:SymbolicAstrologyLens a owl:Class ;
    rdfs:subClassOf :InterpretiveLens ;
    rdfs:label "Symbolic Astrology Lens" ;
    rdfs:comment "Interpretation through astrological symbolism as spiritual initiation" .

:NaturalMagicLens a owl:Class ;
    rdfs:subClassOf :InterpretiveLens ;
    rdfs:label "Natural Magic Lens" ;
    rdfs:comment "Interpretation through psycho-spiritual therapy and sympathetic resonance" .

#################################################################
# Philosophical and Theological Concepts
#################################################################

:PhilosophicalConcept a owl:Class ;
    rdfs:subClassOf dolce:SocialObject ;
    rdfs:label "Philosophical Concept" ;
    rdfs:comment "Abstract philosophical or theological idea manifested in artifact" .

:TheOne a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "The One" ;
    rdfs:comment "The ineffable divine unity transcending all categories (Neo-Platonic)" .

:ImmortalSoul a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Immortal Soul" ;
    rdfs:comment "The eternal aspect of human being transcending material existence" .

:CoincidentiaOppositorum a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Coincidentia Oppositorum" ;
    rdfs:comment "Union of opposites in divine unity" .

:ViaNegativa a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Via Negativa" ;
    rdfs:comment "Apophatic theological method describing the divine through negation" .

:DivineEmanation a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Divine Emanation" ;
    rdfs:comment "Neo-Platonic doctrine of reality flowing from the One through hierarchical levels" .

:SoulsDescent a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Soul's Descent" ;
    rdfs:comment "The descent of soul from divine unity into material manifestation" .

:SoulsAscent a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Soul's Ascent" ;
    rdfs:comment "The soul's journey upward from material world toward divine source" .

:CosmicCycles a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Cosmic Cycles" ;
    rdfs:comment "Doctrine of recurring world ages and celestial transformation" .

#################################################################
# Cosmological Structure and Hierarchy
#################################################################

:CosmologicalStructure a owl:Class ;
    rdfs:subClassOf dolce:Structure ;
    rdfs:label "Cosmological Structure" ;
    rdfs:comment "Hierarchical arrangement reflecting cosmic order" .

:CosmicRealm a owl:Class ;
    rdfs:subClassOf dolce:Region ;
    rdfs:label "Cosmic Realm" ;
    rdfs:comment "Level or plane in the hierarchical cosmology" .

:DivineRealm a owl:Class ;
    rdfs:subClassOf :CosmicRealm ;
    rdfs:label "Divine Realm" ;
    rdfs:comment "Highest level of reality; the realm of divine unity" .

:IntermediaryRealm a owl:Class ;
    rdfs:subClassOf :CosmicRealm ;
    rdfs:label "Intermediary Realm" ;
    rdfs:comment "Middle level of reality; realm of soul, intellect, or celestial beings" .

:MaterialRealm a owl:Class ;
    rdfs:subClassOf :CosmicRealm ;
    rdfs:label "Material Realm" ;
    rdfs:comment "Lowest level of reality; the realm of physical manifestation" .

:CelestialSphere a owl:Class ;
    rdfs:subClassOf :CosmicRealm ;
    rdfs:label "Celestial Sphere" ;
    rdfs:comment "The sphere of stars, planets, and heavenly bodies" .

:TerrestrialSphere a owl:Class ;
    rdfs:subClassOf :CosmicRealm ;
    rdfs:label "Terrestrial Sphere" ;
    rdfs:comment "The earthly or sublunary sphere" .

#################################################################
# Spiritual Seekers and Initiates
#################################################################

:SpiritualSeeker a owl:Class ;
    rdfs:subClassOf dolce:Person ;
    rdfs:label "Spiritual Seeker" ;
    rdfs:comment "Individual pursuing esoteric knowledge and spiritual transformation" .

:PhilosopherInitiate a owl:Class ;
    rdfs:subClassOf :SpiritualSeeker ;
    rdfs:label "Philosopher-Initiate" ;
    rdfs:comment "One who has penetrated the veil and attained cosmological gnosis" .

#################################################################
# Alchemical and Transformative Concepts
#################################################################

:AlchemicalConcept a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Alchemical Concept" ;
    rdfs:comment "Concept related to alchemical transformation and spiritual opus" .

:AlchemicalOpus a owl:Class ;
    rdfs:subClassOf :AlchemicalConcept ;
    rdfs:label "Alchemical Opus" ;
    rdfs:comment "The Great Work of spiritual and material transformation" .

:SpiritualRebirth a owl:Class ;
    rdfs:subClassOf :AlchemicalConcept ;
    rdfs:label "Spiritual Rebirth" ;
    rdfs:comment "Alchemical transformation resulting in renewed spiritual consciousness" .

:InnerTransformation a owl:Class ;
    rdfs:subClassOf :AlchemicalConcept ;
    rdfs:label "Inner Transformation" ;
    rdfs:comment "The psychological and spiritual metamorphosis of the practitioner" .

#################################################################
# Celestial and Astrological Elements
#################################################################

:CelestialInfluence a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Celestial Influence" ;
    rdfs:comment "Spiritual or cosmic force from higher planes affecting material realm" .

:CelestialIntelligence a owl:Class ;
    rdfs:subClassOf dolce:Agent ;
    rdfs:label "Celestial Intelligence" ;
    rdfs:comment "Angelic or planetary spirit governing celestial influences" .

:CelestialHarmony a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Celestial Harmony" ;
    rdfs:comment "Harmonious ordering of celestial spheres and their influences" .

#################################################################
# Ancient Wisdom Sources
#################################################################

:AncientWisdomSource a owl:Class ;
    rdfs:subClassOf dolce:InformationObject ;
    rdfs:label "Ancient Wisdom Source" ;
    rdfs:comment "Traditional text or teaching from ancient sages" .

:HermeticTradition a owl:Class ;
    rdfs:subClassOf :AncientWisdomSource ;
    rdfs:label "Hermetic Tradition" ;
    rdfs:comment "Wisdom attributed to Hermes Trismegistus and related texts" .

:PlatonicTradition a owl:Class ;
    rdfs:subClassOf :AncientWisdomSource ;
    rdfs:label "Platonic Tradition" ;
    rdfs:comment "Philosophical lineage from Plato through Plotinus" .

:ChristianMysticalTradition a owl:Class ;
    rdfs:subClassOf :AncientWisdomSource ;
    rdfs:label "Christian Mystical Tradition" ;
    rdfs:comment "Christian contemplative and mystical theology" .

:AncientSage a owl:Class ;
    rdfs:subClassOf dolce:Person ;
    rdfs:label "Ancient Sage" ;
    rdfs:comment "Recognized bearer of ancient wisdom (e.g., Hermes Trismegistus, Orpheus)" .

#################################################################
# Symbolic Components
#################################################################

:SymbolicComponent a owl:Class ;
    rdfs:subClassOf dolce:PhysicalObject ;
    rdfs:label "Symbolic Component" ;
    rdfs:comment "Physical element of artifact carrying symbolic meaning" .

:CelestialSymbol a owl:Class ;
    rdfs:subClassOf :SymbolicComponent ;
    rdfs:label "Celestial Symbol" ;
    rdfs:comment "Symbolic representation of celestial body or force" .

:StarSymbol a owl:Class ;
    rdfs:subClassOf :CelestialSymbol ;
    rdfs:label "Star Symbol" ;
    rdfs:comment "Symbolic representation of star or stellar influence" .

:MoonSymbol a owl:Class ;
