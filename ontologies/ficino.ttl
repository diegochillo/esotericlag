#################################################################
# Symbolic and Rhetorical Devices
#################################################################

:SymbolicDevice a owl:Class ;
    rdfs:subClassOf dolce:Method ;
    rdfs:label "Symbolic Device" ;
    rdfs:comment "Rhetorical or symbolic technique encoding spiritual meaning" .

:ParadoxicalFormula a owl:Class ;
    rdfs:subClassOf :SymbolicDevice ;
    rdfs:label "Paradoxical Formula" ;
    rdfs:comment "Statement containing apparent contradictions revealing deeper truth" .

:InitiaticSymbol a owl:Class ;
    rdfs:subClassOf :SymbolicDevice ;
    rdfs:label "Initiatic Symbol" ;
    rdfs:comment "Symbol serving as device for spiritual initiation" .

:ContemplativeDevice a owl:Class ;
    rdfs:subClassOf :SymbolicDevice ;
    rdfs:label "Contemplative Device" ;
