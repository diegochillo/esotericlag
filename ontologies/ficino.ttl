#################################################################

:hasStructuralComponent a owl:ObjectProperty ;
    rdfs:domain :EsotericArtifact ;
    rdfs:range :SymbolicComponent ;
    rdfs:label "has structural component" ;
    rdfs:comment "Artifact contains this physical symbolic element" .

:exhibitsCosmologicalStructure a owl:ObjectProperty ;
    rdfs:domain :EsotericArtifact ;
    rdfs:range :CosmologicalStructure ;
    rdfs:label "exhibits cosmological structure" ;
    rdfs:comment "Artifact displays hierarchical cosmic ordering" .

:mirrorsCosmicRealm a owl:ObjectProperty ;
