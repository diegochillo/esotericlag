#################################################################
# Property Characteristics
#################################################################

:ascendsFrom owl:inverseOf :descendsFrom .
:correspondsToDivineLevel rdfs:subPropertyOf :mirrorsCosmicRealm .
:correspondsToIntermediaryLevel rdfs:subPropertyOf :mirrorsCosmicRealm .
:correspondsToMaterialLevel rdfs:subPropertyOf :mirrorsCosmicRealm .

#################################################################
# Class Restrictions
#################################################################

:PhilosopherInitiate rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :graspsTruth ;
    owl:someValuesFrom :PhilosophicalConcept
] .

:EsotericArtifact rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :revealsPhilosophicalConcept ;
    owl:someValuesFrom :PhilosophicalConcept
] .

:FicinianInterpretation rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :appliesLens ;
    owl:someValuesFrom :InterpretiveLens
] .

:SymbolicDevice rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :pointsTowardConcept ;
    owl:someValuesFrom :PhilosophicalConcept
] .

:ContemplativeDevice rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :facilitatesSpiritualState ;
    owl:someValuesFrom :SpiritualState
] .

#################################################################
# Disjointness Axioms
#################################################################

:DivineRealm owl:disjointWith :MaterialRealm .
:PriscaTheologiaLens owl:disjointWith :NeoPlatonismLens .
:PriscaTheologiaLens owl:disjointWith :ChristianIntegrationLens .
:PriscaTheologiaLens owl:disjointWith :SymbolicAstrologyLens .
:PriscaTheologiaLens owl:disjointWith :NaturalMagicLens .
:NeoPlatonismLens owl:disjointWith :ChristianIntegrationLens .
:NeoPlatonismLens owl:disjointWith :SymbolicAstrologyLens .
:NeoPlatonismLens owl:disjointWith :NaturalMagicLens .
:ChristianIntegrationLens owl:disjointWith :SymbolicAstrologyLens .
:ChristianIntegrationLens owl:disjointWith :NaturalMagicLens .
:SymbolicAstrologyLens owl:disjointWith :NaturalMagicLens .

