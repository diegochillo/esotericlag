    rdfs:label "Moon Symbol" ;
    rdfs:comment "Symbolic representation of lunar influence" .

:SunSymbol a owl:Class ;
    rdfs:subClassOf :CelestialSymbol ;
    rdfs:label "Sun Symbol" ;
