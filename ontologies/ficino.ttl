:MysticalGnosis a owl:Class ;
    rdfs:subClassOf :SpiritualState ;
    rdfs:label "Mystical Gnosis" ;
    rdfs:comment "Direct spiritual knowledge transcending rational understanding" .

:TranscendenceBeyondCategories a owl:Class ;
    rdfs:subClassOf :SpiritualState ;
    rdfs:label "Transcendence Beyond Categories" ;
    rdfs:comment "State of consciousness surpassing material distinctions" .

:UnioMystica a owl:Class ;
    rdfs:subClassOf :SpiritualState ;
    rdfs:label "Unio Mystica" ;
    rdfs:comment "Mystical union with the divine" .

:SpiritualAscent a owl:Class ;
    rdfs:subClassOf dolce:Process ;
    rdfs:label "Spiritual Ascent" ;
    rdfs:comment "Process of soul's journey toward divine union" .

:SpiritualLiberation a owl:Class ;
    rdfs:subClassOf :SpiritualState ;
    rdfs:label "Spiritual Liberation" ;
    rdfs:comment "Freedom from cosmic determinism through gnosis" .

:SympatheticAlignment a owl:Class ;
    rdfs:subClassOf dolce:Process ;
    rdfs:label "Sympathetic Alignment" ;
    rdfs:comment "Process of attuning soul to celestial intelligences" .

#################################################################
# Synthesis Concepts
#################################################################

:ChristianHermeticism a owl:Class ;
    rdfs:subClassOf :PhilosophicalConcept ;
    rdfs:label "Christian Hermeticism" ;
    rdfs:comment "Synthesis of Hermetic philosophy with Christian theology" .

#################################################################
# Properties - Interpretation Relations
#################################################################

:interpretsArtifact a owl:ObjectProperty ;
    rdfs:domain :FicinianInterpretation ;
    rdfs:range :EsotericArtifact ;
    rdfs:label "interprets artifact" ;
    rdfs:comment "Links interpretation to the artifact being analyzed" .

:appliesLens a owl:ObjectProperty ;
    rdfs:domain :FicinianInterpretation ;
    rdfs:range :InterpretiveLens ;
    rdfs:label "applies lens" ;
    rdfs:comment "Indicates which interpretive lens is used in analysis" .

:revealsPhilosophicalConcept a owl:ObjectProperty ;
    rdfs:domain :EsotericArtifact ;
    rdfs:range :PhilosophicalConcept ;
    rdfs:label "reveals philosophical concept" ;
    rdfs:comment "Artifact encodes or manifests this concept" .

:referencesAncientSource a owl:ObjectProperty ;
    rdfs:domain :EsotericArtifact ;
    rdfs:range :AncientWisdomSource ;
    rdfs:label "references ancient source" ;
    rdfs:comment "Artifact draws from or alludes to ancient wisdom tradition" .

:attributedToSage a owl:ObjectProperty ;
    rdfs:domain :AncientWisdomSource ;
    rdfs:range :AncientSage ;
    rdfs:label "attributed to sage" ;
    rdfs:comment "Wisdom is attributed to this ancient sage" .

