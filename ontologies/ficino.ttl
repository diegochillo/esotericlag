:representsEmanation a owl:ObjectProperty ;
    rdfs:domain :CosmologicalStructure ;
    rdfs:range :DivineEmanation ;
    rdfs:label "represents emanation" ;
    rdfs:comment "Structure depicts the process of divine emanation" .

:symbolizesDescent a owl:ObjectProperty ;
    rdfs:domain :CosmologicalStructure ;
    rdfs:range :SoulsDescent ;
    rdfs:label "symbolizes descent" ;
    rdfs:comment "Structure represents the soul's descent into matter" .

:symbolizesAscent a owl:ObjectProperty ;
    rdfs:domain :CosmologicalStructure ;
    rdfs:range :SoulsAscent ;
    rdfs:label "symbolizes ascent" ;
    rdfs:comment "Structure represents the soul's ascent toward divine" .

