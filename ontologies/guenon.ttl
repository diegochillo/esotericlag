@prefix : <http://guenon.org/ontology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dolce: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@base <http://guenon.org/ontology#> .

<http://guenon.org/ontology> rdf:type owl:Ontology ;
                              owl:imports <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl> ;
                              dc:description "A synthesized ontology for analyzing esoteric artifacts according to René Guénon's traditional metaphysical framework"@en ;
                              dc:title "Guénonian Esoteric Artifact Analysis Ontology - Core Framework"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://guenon.org/ontology#appliesMethod
:appliesMethod rdf:type owl:ObjectProperty ;
               rdfs:domain :GuénonianAssessment ;
               rdfs:range :AnalyticalMethod ;
               rdfs:comment "Indicates analytical method used in assessment"@en ;
               rdfs:label "applies method"@en .


###  http://guenon.org/ontology#assessesArtifact
:assessesArtifact rdf:type owl:ObjectProperty ;
                  rdfs:domain :GuénonianAssessment ;
                  rdfs:range :EsotericArtifact ;
                  rdfs:comment "Links assessment to the artifact being evaluated"@en ;
                  rdfs:label "assesses artifact"@en .


###  http://guenon.org/ontology#compares
:compares rdf:type owl:ObjectProperty ;
          rdfs:domain :ComparativeAssessment ;
          rdfs:range :EsotericArtifact ;
          rdfs:comment "Links comparative assessment to artifacts being compared"@en ;
          rdfs:label "compares"@en .


###  http://guenon.org/ontology#containsSymbol
:containsSymbol rdf:type owl:ObjectProperty ;
                rdfs:domain :EsotericArtifact ;
                rdfs:range :TraditionalSymbol ;
                rdfs:comment "Indicates a symbol present in an artifact"@en ;
                rdfs:label "contains symbol"@en .


###  http://guenon.org/ontology#correspondsWith
:correspondsWith rdf:type owl:ObjectProperty ;
                 rdfs:domain :TraditionalSymbol ;
                 rdfs:range :MetaphysicalPrinciple ;
                 rdfs:comment "Indicates symbolic correspondence with a principle"@en ;
                 rdfs:label "corresponds with"@en .


###  http://guenon.org/ontology#datesTo
:datesTo rdf:type owl:ObjectProperty ;
         rdfs:domain :EsotericArtifact ;
         rdfs:range :TemporalCategory ;
         rdfs:comment "Indicates temporal period of artifact"@en ;
         rdfs:label "dates to"@en .


###  http://guenon.org/ontology#demonstrablyEmbodiesPrinciple
:demonstrablyEmbodiesPrinciple rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :embodiesPrinciple ;
                               rdfs:comment "Indicates metaphysical principle with strong evidence"@en ;
                               rdfs:label "demonstrably embodies principle"@en .


###  http://guenon.org/ontology#derivesPrinciple
:derivesPrinciple rdf:type owl:ObjectProperty ;
                  rdfs:domain :TraditionalLineage ;
                  rdfs:range :PrimordialTradition ;
                  rdfs:comment "Indicates derivation from primordial tradition"@en ;
                  rdfs:label "derives principle"@en .


###  http://guenon.org/ontology#embodiesPrinciple
:embodiesPrinciple rdf:type owl:ObjectProperty ;
                   rdfs:domain :EsotericArtifact ;
                   rdfs:range :MetaphysicalPrinciple ;
                   rdfs:comment "Indicates a metaphysical principle manifested in an artifact"@en ;
                   rdfs:label "embodies principle"@en .


###  http://guenon.org/ontology#fabricatesConnection
:fabricatesConnection rdf:type owl:ObjectProperty ;
                      rdfs:domain :EsotericArtifact ;
                      rdfs:range :FabricatedConnection ;
                      rdfs:comment "Indicates false or artificial connection to principles"@en ;
                      rdfs:label "fabricates connection"@en .


###  http://guenon.org/ontology#hasAuthenticityRating
:hasAuthenticityRating rdf:type owl:ObjectProperty ;
                       rdfs:domain :EsotericArtifact ;
                       rdfs:range :AuthenticitySpectrum ;
                       rdfs:comment "Indicates position on authenticity spectrum"@en ;
                       rdfs:label "has authenticity rating"@en .


###  http://guenon.org/ontology#hasEsotericLayer
:hasEsotericLayer rdf:type owl:ObjectProperty ;
                  rdfs:domain :EsotericArtifact ;
                  rdfs:range :EsotericDimension ;
                  rdfs:comment "Indicates the esoteric aspect of an artifact"@en ;
                  rdfs:label "has esoteric layer"@en .


###  http://guenon.org/ontology#hasExotericLayer
:hasExotericLayer rdf:type owl:ObjectProperty ;
                  rdfs:domain :EsotericArtifact ;
                  rdfs:range :ExotericDimension ;
                  rdfs:comment "Indicates the exoteric aspect of an artifact"@en ;
                  rdfs:label "has exoteric layer"@en .


###  http://guenon.org/ontology#hasQuality
:hasQuality rdf:type owl:ObjectProperty ;
            rdfs:domain :EsotericArtifact ;
            rdfs:range :EssentialQuality ;
            rdfs:comment "Indicates an essential quality of an artifact"@en ;
            rdfs:label "has quality"@en .


###  http://guenon.org/ontology#hasReconstructiveEsotericLayer
:hasReconstructiveEsotericLayer rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf :hasEsotericLayer ;
                                rdfs:domain :EsotericArtifact ;
                                rdfs:range :ReconstructiveEsotericLayer ;
                                rdfs:comment "Indicates esoteric dimension reconstructed without direct documentation"@en ;
                                rdfs:label "has reconstructive esoteric layer"@en .


###  http://guenon.org/ontology#imposesCorrespondence
:imposesCorrespondence rdf:type owl:ObjectProperty ;
                       rdfs:domain :EsotericArtifact ;
                       rdfs:range :ImposedCorrespondence ;
                       rdfs:comment "Indicates interpretively imposed rather than authentic macrocosm/microcosm relationship"@en ;
                       rdfs:label "imposes correspondence"@en .


###  http://guenon.org/ontology#interpretedBy
:interpretedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :EsotericArtifact ;
               rdfs:range :ModernInterpretation ;
               rdfs:comment "Indicates a modern interpretation applied to artifact"@en ;
               rdfs:label "interpreted by"@en .


###  http://guenon.org/ontology#lacksConnection
:lacksConnection rdf:type owl:ObjectProperty ;
                 rdfs:domain :EsotericArtifact ;
                 rdfs:range :AbsentConnection ;
                 rdfs:comment "Indicates complete absence of authentic metaphysical connection"@en ;
                 rdfs:label "lacks connection"@en .


###  http://guenon.org/ontology#lacksEsotericDimension
:lacksEsotericDimension rdf:type owl:ObjectProperty ;
                        rdfs:domain :EsotericArtifact ;
                        rdfs:range :AbsentEsotericDimension ;
                        rdfs:comment "Indicates complete absence of authentic esoteric content"@en ;
                        rdfs:label "lacks esoteric dimension"@en .


###  http://guenon.org/ontology#lacksLineage
:lacksLineage rdf:type owl:ObjectProperty ;
              rdfs:domain :EsotericArtifact ;
              rdfs:range :AbsentLineage ;
              rdfs:comment "Indicates complete absence of traditional transmission"@en ;
              rdfs:label "lacks lineage"@en .


###  http://guenon.org/ontology#lacksSourceSupport
:lacksSourceSupport rdf:type owl:ObjectProperty ;
                    rdfs:domain :EsotericArtifact ;
                    rdfs:range :AbsentCommentary ;
                    rdfs:comment "Indicates absence of traditional commentary"@en ;
                    rdfs:label "lacks source support"@en .


###  http://guenon.org/ontology#originatesFrom
:originatesFrom rdf:type owl:ObjectProperty ;
                rdfs:domain :EsotericArtifact ;
                rdfs:range :TraditionalLineage ;
                rdfs:comment "Indicates the traditional lineage from which an artifact derives"@en ;
                rdfs:label "originates from"@en .


###  http://guenon.org/ontology#possiblyOriginatesFrom
:possiblyOriginatesFrom rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :originatesFrom ;
                        rdfs:domain :EsotericArtifact ;
                        rdfs:range :TraditionalLineage ;
                        rdfs:comment "Indicates uncertain traditional origin"@en ;
                        rdfs:label "possibly originates from"@en .


###  http://guenon.org/ontology#reflectsMacrocosm
:reflectsMacrocosm rdf:type owl:ObjectProperty ;
                   rdfs:domain :MicrocosmicReality ;
                   rdfs:range :MacrocosmicReality ;
                   rdfs:comment "Indicates macrocosmic reality reflected in microcosm"@en ;
                   rdfs:label "reflects macrocosm"@en .


###  http://guenon.org/ontology#supportedByArchaeology
:supportedByArchaeology rdf:type owl:ObjectProperty ;
                        rdfs:domain :EsotericArtifact ;
                        rdfs:range :ArchaeologicalEvidence ;
                        rdfs:comment "Indicates archaeological evidence for artifact"@en ;
                        rdfs:label "supported by archaeology"@en .


###  http://guenon.org/ontology#supportedBySource
:supportedBySource rdf:type owl:ObjectProperty ;
                   rdfs:domain :EsotericArtifact ;
                   rdfs:range :TraditionalSource ;
                   rdfs:comment "Indicates documentary evidence supporting interpretation"@en ;
                   rdfs:label "supported by source"@en .


###  http://guenon.org/ontology#supportsIntuition
:supportsIntuition rdf:type owl:ObjectProperty ;
                   rdfs:domain :TraditionalSymbol ;
                   rdfs:range :IntellectualIntuition ;
                   rdfs:comment "Indicates that a symbol serves as support for intellectual intuition"@en ;
                   rdfs:label "supports intuition"@en .


###  http://guenon.org/ontology#transmittedThrough
:transmittedThrough rdf:type owl:ObjectProperty ;
                    rdfs:domain :MetaphysicalPrinciple ;
                    rdfs:range :DocumentedTransmission ;
                    rdfs:comment "Indicates transmission event for traditional knowledge"@en ;
                    rdfs:label "transmitted through"@en .


#################################################################
#    Data properties
#################################################################

###  http://guenon.org/ontology#hasAuthenticityStatus
:hasAuthenticityStatus rdf:type owl:DatatypeProperty ;
                       rdfs:domain :EsotericArtifact ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Indicates assessment of traditional authenticity"@en ;
                       rdfs:label "has authenticity status"@en .


###  http://guenon.org/ontology#hasCreationPeriod
:hasCreationPeriod rdf:type owl:DatatypeProperty ;
                   rdfs:domain :EsotericArtifact ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Historical period of artifact creation"@en ;
                   rdfs:label "has creation period"@en .


###  http://guenon.org/ontology#hasDocumentationDate
:hasDocumentationDate rdf:type owl:DatatypeProperty ;
                      rdfs:domain :TraditionalSource ;
                      rdfs:range xsd:dateTime ;
                      rdfs:comment "Date of source documentation"@en ;
                      rdfs:label "has documentation date"@en .


###  http://guenon.org/ontology#hasEvidenceLevel
:hasEvidenceLevel rdf:type owl:DatatypeProperty ;
                  rdfs:domain :EsotericArtifact ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates level of documentary evidence (documented/uncertain/absent)"@en ;
                  rdfs:label "has evidence level"@en .


###  http://guenon.org/ontology#hasExotericEsotericVerdict
:hasExotericEsotericVerdict rdf:type owl:DatatypeProperty ;
                            rdfs:domain :GuénonianAssessment ;
                            rdfs:range xsd:string ;
                            rdfs:comment "Assessment of documented evidence for exoteric and esoteric layers"@en ;
                            rdfs:label "has exoteric-esoteric verdict"@en .


###  http://guenon.org/ontology#hasFinalVerdict
:hasFinalVerdict rdf:type owl:DatatypeProperty ;
                 rdfs:domain :GuénonianAssessment ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Overall Guénonian verdict on artifact's traditional authenticity"@en ;
                 rdfs:label "has final verdict"@en .


###  http://guenon.org/ontology#hasInterpretationDate
:hasInterpretationDate rdf:type owl:DatatypeProperty ;
                       rdfs:domain :ModernInterpretation ;
                       rdfs:range xsd:dateTime ;
                       rdfs:comment "Date when modern interpretation was produced"@en ;
                       rdfs:label "has interpretation date"@en .


###  http://guenon.org/ontology#hasMacrocosmMicrocosmVerdict
:hasMacrocosmMicrocosmVerdict rdf:type owl:DatatypeProperty ;
                              rdfs:domain :GuénonianAssessment ;
                              rdfs:range xsd:string ;
                              rdfs:comment "Assessment of demonstrable macrocosm/microcosm correspondence"@en ;
                              rdfs:label "has macrocosm-microcosm verdict"@en .


###  http://guenon.org/ontology#hasMetaQuestionVerdict
:hasMetaQuestionVerdict rdf:type owl:DatatypeProperty ;
                        rdfs:domain :GuénonianAssessment ;
                        rdfs:range xsd:string ;
                        rdfs:comment "Assessment of artifact's origin from authentic traditional lineage"@en ;
                        rdfs:label "has meta-question verdict"@en .


###  http://guenon.org/ontology#hasMetaphysicalPrincipleVerdict
:hasMetaphysicalPrincipleVerdict rdf:type owl:DatatypeProperty ;
                                 rdfs:domain :GuénonianAssessment ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "Assessment of connection to universal transcendent principles"@en ;
                                 rdfs:label "has metaphysical principle verdict"@en .


###  http://guenon.org/ontology#hasQualitativeAnalysisVerdict
:hasQualitativeAnalysisVerdict rdf:type owl:DatatypeProperty ;
                               rdfs:domain :GuénonianAssessment ;
                               rdfs:range xsd:string ;
                               rdfs:comment "Assessment of essential qualities traceable to authentic traditional teachings"@en ;
                               rdfs:label "has qualitative analysis verdict"@en .


###  http://guenon.org/ontology#hasSymbolismVerdict
:hasSymbolismVerdict rdf:type owl:DatatypeProperty ;
                     rdfs:domain :GuénonianAssessment ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Assessment of symbols correspondence to documented traditional forms"@en ;
                     rdfs:label "has symbolism verdict"@en .


###  http://guenon.org/ontology#isDocumented
:isDocumented rdf:type owl:DatatypeProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range xsd:boolean ;
              rdfs:comment "Indicates whether claim has documentary support"@en ;
              rdfs:label "is documented"@en .


###  http://guenon.org/ontology#isSpeculative
:isSpeculative rdf:type owl:DatatypeProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range xsd:boolean ;
               rdfs:comment "Indicates whether interpretation is speculative"@en ;
               rdfs:label "is speculative"@en .


###  http://guenon.org/ontology#locationName
:locationName rdf:type owl:DatatypeProperty ;
              rdfs:domain :EsotericArtifact ;
              rdfs:range xsd:string ;
              rdfs:comment "Geographic location of artifact"@en ;
              rdfs:label "location name"@en .


#################################################################
#    Classes
#################################################################

###  http://guenon.org/ontology#AbsentCommentary
:AbsentCommentary rdf:type owl:Class ;
                  rdfs:subClassOf dolce:InformationObject ;
                  rdfs:comment "Lack of documented traditional commentary on an artifact"@en ;
                  rdfs:label "Absent Commentary"@en .


###  http://guenon.org/ontology#AbsentConnection
:AbsentConnection rdf:type owl:Class ;
                  rdfs:subClassOf dolce:Description ;
                  rdfs:comment "Complete lack of connection to authentic metaphysical principles"@en ;
                  rdfs:label "Absent Connection"@en .


###  http://guenon.org/ontology#AbsentEsotericDimension
:AbsentEsotericDimension rdf:type owl:Class ;
                         rdfs:subClassOf dolce:Description ;
                         rdfs:comment "Complete lack of authentic esoteric content"@en ;
                         rdfs:label "Absent Esoteric Dimension"@en .


###  http://guenon.org/ontology#AbsentLineage
:AbsentLineage rdf:type owl:Class ;
               rdfs:subClassOf dolce:SocialObject ;
               rdfs:comment "Complete lack of traditional transmission chain"@en ;
               rdfs:label "Absent Lineage"@en .


###  http://guenon.org/ontology#AnalogicalCorrespondence
:AnalogicalCorrespondence rdf:type owl:Class ;
                          rdfs:subClassOf dolce:Relation ;
                          rdfs:comment "A relationship of analogy between different levels of reality"@en ;
                          rdfs:label "Analogical Correspondence"@en .


###  http://guenon.org/ontology#AnalyticalMethod
:AnalyticalMethod rdf:type owl:Class ;
                  rdfs:subClassOf dolce:Method ;
                  rdfs:comment "A method for analyzing esoteric artifacts"@en ;
                  rdfs:label "Analytical Method"@en .


###  http://guenon.org/ontology#AncientArtifact
:AncientArtifact rdf:type owl:Class ;
                 rdfs:subClassOf :EsotericArtifact ;
                 owl:disjointWith :ModernArtifact ;
                 rdfs:comment "An artifact of demonstrable antiquity predating modern era"@en ;
                 rdfs:label "Ancient Artifact"@en .


###  http://guenon.org/ontology#ArchaeologicalEvidence
:ArchaeologicalEvidence rdf:type owl:Class ;
                        rdfs:subClassOf dolce:InformationObject ;
                        rdfs:comment "Physical evidence from archaeological discovery"@en ;
                        rdfs:label "Archaeological Evidence"@en .


###  http://guenon.org/ontology#AuthenticSymbol
:AuthenticSymbol rdf:type owl:Class ;
                 rdfs:subClassOf :TraditionalSymbol ;
                 rdfs:comment "A symbol verified as part of an authentic traditional lineage"@en ;
                 rdfs:label "Authentic Symbol"@en .


###  http://guenon.org/ontology#AuthenticTraditionalArtifact
:AuthenticTraditionalArtifact rdf:type owl:Class ;
                              rdfs:subClassOf :EsotericArtifact ;
                              rdfs:comment "An artifact originating from a documented authentic traditional lineage"@en ;
                              rdfs:label "Authentic Traditional Artifact"@en .


###  http://guenon.org/ontology#AuthenticitySpectrum
:AuthenticitySpectrum rdf:type owl:Class ;
                      rdfs:subClassOf dolce:Quality ;
                      rdfs:comment "Spectrum from authentic traditional to pseudo-traditional"@en ;
                      rdfs:label "Authenticity Spectrum"@en .


###  http://guenon.org/ontology#ComparativeAssessment
:ComparativeAssessment rdf:type owl:Class ;
                       rdfs:subClassOf dolce:Description ;
                       rdfs:comment "Comparison of multiple artifacts according to Guénonian criteria"@en ;
                       rdfs:label "Comparative Assessment"@en .


###  http://guenon.org/ontology#CompetencyQuestion
:CompetencyQuestion rdf:type owl:Class ;
                    rdfs:subClassOf dolce:InformationObject ;
                    rdfs:comment "A question this ontology is designed to answer"@en ;
                    rdfs:label "Competency Question"@en .


###  http://guenon.org/ontology#CosmicPrinciple
:CosmicPrinciple rdf:type owl:Class ;
                 rdfs:subClassOf :MetaphysicalPrinciple ;
                 rdfs:comment "A principle governing cosmic manifestation"@en ;
                 rdfs:label "Cosmic Principle"@en .


###  http://guenon.org/ontology#CosmicSymbolism
:CosmicSymbolism rdf:type owl:Class ;
                 rdfs:subClassOf :TraditionalSymbol ;
                 rdfs:comment "Symbolism directly reflecting cosmic structures and principles"@en ;
                 rdfs:label "Cosmic Symbolism"@en .


###  http://guenon.org/ontology#DesignPattern
:DesignPattern rdf:type owl:Class ;
               rdfs:subClassOf dolce:Description ;
               rdfs:comment "A reusable ontology design pattern"@en ;
               rdfs:label "Design Pattern"@en .


###  http://guenon.org/ontology#DocumentedEsotericLayer
:DocumentedEsotericLayer rdf:type owl:Class ;
                         rdfs:subClassOf :EsotericDimension ;
                         rdfs:comment "An esoteric interpretation verified by traditional sources"@en ;
                         rdfs:label "Documented Esoteric Layer"@en .


###  http://guenon.org/ontology#DocumentedTransmission
:DocumentedTransmission rdf:type owl:Class ;
                        rdfs:subClassOf dolce:Event ;
                        rdfs:comment "A verifiable act of transmission within a traditional lineage"@en ;
                        rdfs:label "Documented Transmission"@en .


###  http://guenon.org/ontology#EclecticBorrowing
:EclecticBorrowing rdf:type owl:Class ;
                   rdfs:subClassOf dolce:InformationObject ;
                   rdfs:comment "A symbol assembled from disparate sources without organic unity"@en ;
                   rdfs:label "Eclectic Borrowing"@en .


###  http://guenon.org/ontology#EsotericArtifact
:EsotericArtifact rdf:type owl:Class ;
                  rdfs:subClassOf dolce:PhysicalObject ;
                  rdfs:comment "A physical object subject to traditional esoteric analysis according to Guénon's framework"@en ;
                  rdfs:label "Esoteric Artifact"@en .


###  http://guenon.org/ontology#EsotericDimension
:EsotericDimension rdf:type owl:Class ;
                   rdfs:subClassOf dolce:Description ;
                   rdfs:comment "The inner, initiatic aspect requiring special qualification"@en ;
                   rdfs:label "Esoteric Dimension"@en .


###  http://guenon.org/ontology#EssentialQuality
:EssentialQuality rdf:type owl:Class ;
                  rdfs:subClassOf dolce:Quality ;
                  rdfs:comment "A quality relating to essential nature rather than quantitative measure"@en ;
                  rdfs:label "Essential Quality"@en .


###  http://guenon.org/ontology#EvidenceLevel
:EvidenceLevel rdf:type owl:Class ;
               rdfs:subClassOf dolce:Quality ;
               rdfs:comment "The degree of documentary support for a claim"@en ;
               rdfs:label "Evidence Level"@en .


###  http://guenon.org/ontology#ExotericDimension
:ExotericDimension rdf:type owl:Class ;
                   rdfs:subClassOf dolce:Description ;
                   rdfs:comment "The outer, accessible aspect of traditional teaching"@en ;
                   rdfs:label "Exoteric Dimension"@en .


###  http://guenon.org/ontology#ExtensionPoint
:ExtensionPoint rdf:type owl:Class ;
                rdfs:subClassOf dolce:Description ;
                rdfs:comment "A point where this core ontology can be extended for specific cases"@en ;
                rdfs:label "Extension Point"@en .


###  http://guenon.org/ontology#FabricatedConnection
:FabricatedConnection rdf:type owl:Class ;
                      rdfs:subClassOf dolce:Description ;
                      rdfs:comment "A false or artificial connection to metaphysical principles"@en ;
                      rdfs:label "Fabricated Connection"@en .


###  http://guenon.org/ontology#GuénonianAssessment
:GuénonianAssessment rdf:type owl:Class ;
                     rdfs:subClassOf dolce:Description ;
                     rdfs:comment "A comprehensive assessment following Guénon's analytical framework"@en ;
                     rdfs:label "Guénonian Assessment"@en .


###  http://guenon.org/ontology#GuénonianDistinction
:GuénonianDistinction rdf:type owl:Class ;
                      rdfs:subClassOf dolce:Concept ;
                      rdfs:comment "A fundamental conceptual distinction in Guénon's framework"@en ;
                      rdfs:label "Guénonian Distinction"@en .


###  http://guenon.org/ontology#GuénonianMethod
:GuénonianMethod rdf:type owl:Class ;
                 rdfs:subClassOf :AnalyticalMethod ;
                 rdfs:comment "René Guénon's five-point analytical framework for assessing traditional authenticity"@en ;
                 rdfs:label "Guénonian Method"@en .


###  http://guenon.org/ontology#HighAuthenticity
:HighAuthenticity rdf:type owl:Class ;
                  rdfs:subClassOf :AuthenticitySpectrum ;
                  rdfs:comment "Strong evidence of authentic traditional lineage"@en ;
                  rdfs:label "High Authenticity"@en .


###  http://guenon.org/ontology#IdiosyncraticArtifact
:IdiosyncraticArtifact rdf:type owl:Class ;
                       rdfs:subClassOf :EsotericArtifact ;
                       rdfs:comment "An artifact expressing personal or local interpretations rather than traditional forms"@en ;
                       rdfs:label "Idiosyncratic Artifact"@en .


###  http://guenon.org/ontology#ImposedCorrespondence
:ImposedCorrespondence rdf:type owl:Class ;
                       rdfs:subClassOf dolce:Relation ;
                       rdfs:comment "Macrocosm/microcosm relationship imposed through modern interpretation rather than traditional teaching"@en ;
                       rdfs:label "Imposed Correspondence"@en .


###  http://guenon.org/ontology#InitiaticLineage
:InitiaticLineage rdf:type owl:Class ;
                  rdfs:subClassOf :TraditionalLineage ;
                  rdfs:comment "A lineage transmitting authentic initiation and esoteric realization"@en ;
                  rdfs:label "Initiatic Lineage"@en .


###  http://guenon.org/ontology#IntellectualIntuition
:IntellectualIntuition rdf:type owl:Class ;
                       rdfs:subClassOf dolce:Concept ;
                       rdfs:comment "Supra-rational direct apprehension of metaphysical truth"@en ;
                       rdfs:label "Intellectual Intuition"@en .


###  http://guenon.org/ontology#LowAuthenticity
:LowAuthenticity rdf:type owl:Class ;
                 rdfs:subClassOf :AuthenticitySpectrum .


###  http://guenon.org/ontology#MacrocosmicReality
:MacrocosmicReality rdf:type owl:Class ;
                    rdfs:subClassOf dolce:PhysicalObject ;
                    rdfs:comment "The greater cosmic reality reflected in lesser forms"@en ;
                    rdfs:label "Macrocosmic Reality"@en .


###  http://guenon.org/ontology#MetaphysicalPrinciple
:MetaphysicalPrinciple rdf:type owl:Class ;
                       rdfs:subClassOf dolce:Abstract ;
                       rdfs:comment "A universal, transcendent principle beyond individual manifestation"@en ;
                       rdfs:label "Metaphysical Principle"@en .


###  http://guenon.org/ontology#MicrocosmicReality
:MicrocosmicReality rdf:type owl:Class ;
                    rdfs:subClassOf dolce:PhysicalObject ;
                    rdfs:comment "A lesser reality reflecting the greater cosmic structure"@en ;
